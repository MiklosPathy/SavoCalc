<Window x:Class="SavoUI.MainWindow"
        xmlns="http://schemas.microsoft.com/winfx/2006/xaml/presentation"
        xmlns:x="http://schemas.microsoft.com/winfx/2006/xaml"
        xmlns:src="clr-namespace:SavoUI"
        xmlns:scl="clr-namespace:SavoCalcLib;assembly=SavoCalcLib"
        xmlns:themes="clr-namespace:WPF.Themes;assembly=WPF.Themes"
    themes:ThemeManager.Theme="{Binding ElementName=themes, Path=SelectedItem}"
    Loaded="Window_Loaded"
    Background="{DynamicResource WindowBackgroundBrush}"
      
        
        Title="SavoCalc .NET" Height="380" Width="525" FontWeight="Normal">
    <Window.Resources>
        <scl:SavoCalc x:Key="savocalc" />
        <src:FloatConsolidator x:Key="FloatConsolidator" />
    </Window.Resources>


    <Grid Name="RootGrid">

        <Grid.RowDefinitions>
            <RowDefinition Height="Auto" ></RowDefinition>
            <RowDefinition Height="1*"></RowDefinition>
        </Grid.RowDefinitions>

        <Grid Grid.Row="0" >
            <Grid.ColumnDefinitions>
                <ColumnDefinition Width="1*" />
                <ColumnDefinition Width="1*" />
                <ColumnDefinition Width="1*" />
            </Grid.ColumnDefinitions>

            <Grid>
                <Grid.ColumnDefinitions>
                    <ColumnDefinition Width="Auto"/>
                    <ColumnDefinition Width="1*"/>
                </Grid.ColumnDefinitions>
                <Label Content="Téma: " Grid.Column="0"/>
                <ComboBox
                    Grid.Column="1"
                    MaxHeight="24" 
                    x:Name="themes" 
                    SelectionChanged="themes_SelectionChanged" 
                    SelectedIndex="9" />
            </Grid>

            <Grid Grid.Column="1">
                <Grid.ColumnDefinitions>
                    <ColumnDefinition Width="Auto"/>
                    <ColumnDefinition Width="1*"/>
                </Grid.ColumnDefinitions>
                <Label 
                    Content="Zoom:"
                    Grid.Column="0"/>
                <Slider
                    Name="sliderZoom" 
                    Minimum=".5" 
                    Value="1" 
                    Maximum="3"
                    Grid.Column="1"
                    />
            </Grid>
            <Label Content="SavoCalc .NET 1.0" Grid.Column="2" HorizontalAlignment="Right" FontWeight="Normal" />
        </Grid>

        <Grid Name="maingrid" Grid.Row="1">

            <Grid.LayoutTransform>
                <ScaleTransform 
                ScaleX="{Binding ElementName=sliderZoom,Path=Value}" 
                ScaleY="{Binding ElementName=sliderZoom,Path=Value}"/>
            </Grid.LayoutTransform>

            <Grid.RowDefinitions>
                <RowDefinition Height="1*"></RowDefinition>
                <RowDefinition Height="Auto"></RowDefinition>
            </Grid.RowDefinitions>

            <Grid Name="Felső_egyebek" Grid.Row="0">
                <Grid.ColumnDefinitions>
                    <ColumnDefinition Width="1*" />
                    <ColumnDefinition Width="1*" />
                </Grid.ColumnDefinitions>
                <Grid Grid.Column="0">
                    <Grid.ColumnDefinitions>
                        <ColumnDefinition Width="Auto" />
                        <ColumnDefinition Width="1*" />
                    </Grid.ColumnDefinitions>
                    <Grid.RowDefinitions>
                        <RowDefinition Height="Auto" />
                        <RowDefinition Height="1*" />
                    </Grid.RowDefinitions>

                    <Slider Name="slHeight_cm" 
                        Orientation="Vertical" 
                        Grid.Row="1" 
                        Grid.Column="0"              
                        Minimum="1" 
                        Maximum="1000"
                        SmallChange="1"
                        LargeChange="25"
                        IsSnapToTickEnabled="True"
                        Value="{Binding Source={StaticResource savocalc}, Path=Height_cm}"
                        />
                    <Slider Name="slWidth_cm"
                        Grid.Row="0"
                        Grid.Column="1" 
                        Minimum="1" 
                        Maximum="1000"
                        SmallChange="1"
                        LargeChange="25"
                        IsSnapToTickEnabled="True"
                        Value="{Binding Source={StaticResource savocalc}, Path=Width_cm}"
                        />


                    <Grid Grid.Column="1" Grid.Row="1">
                        <StackPanel>
                            <Grid Grid.Row="1" Grid.Column="1">
                                <Grid.ColumnDefinitions>
                                    <ColumnDefinition Width="1*" />
                                    <ColumnDefinition Width="1*" />
                                    <ColumnDefinition Width="1*" />
                                </Grid.ColumnDefinitions>
                                <Grid.RowDefinitions>
                                    <RowDefinition Height="Auto" />
                                    <RowDefinition Height="Auto" />
                                    <RowDefinition Height="Auto" />
                                    <RowDefinition Height="Auto" />
                                    <RowDefinition Height="Auto" />
                                    <RowDefinition Height="1*" />
                                </Grid.RowDefinitions>
                                <Label Content="Szélesség:" Grid.Row="0" Grid.Column="0" />
                                <Label Content="Magasság:" Grid.Row="1" Grid.Column="0" />
                                <Label Content="Felület:" Grid.Row="2" Grid.Column="0" />
                                <Label Content="Hatásfok:" Grid.Row="3" Grid.Column="0" />
                                <StackPanel Orientation="Horizontal" Grid.Row="0" Grid.Column="1">
                                    <Label 
                                Name="tbWidth_cm" 
                                Content="{Binding Source={StaticResource savocalc}, Path=Width_cm}" FontWeight="Bold" />
                                    <Label Content=" cm" />
                                </StackPanel>
                                <StackPanel Orientation="Horizontal" Grid.Row="0" Grid.Column="2">
                                    <Label 
                                Name="tbWidth_m" 
                                Content="{Binding Source={StaticResource savocalc}, Path=Width_m}" FontWeight="Bold" />
                                    <Label Content=" m" />
                                </StackPanel>
                                <StackPanel Orientation="Horizontal" Grid.Row="1" Grid.Column="1">
                                    <Label 
                                Name="tbHeight_cm" 
                                Content="{Binding Source={StaticResource savocalc}, Path=Height_cm}" FontWeight="Bold" />
                                    <Label Content=" cm" />
                                </StackPanel>
                                <StackPanel Orientation="Horizontal" Grid.Row="1" Grid.Column="2">
                                    <Label 
                                Name="tbHeight_m"
                                Content="{Binding Source={StaticResource savocalc}, Path=Height_m}" FontWeight="Bold" />
                                    <Label Content=" m" />
                                </StackPanel>
                                <StackPanel Orientation="Horizontal" Grid.Row="2" Grid.Column="1">
                                    <Label 
                                Name="tbSurface_cm2"
                                Content="{Binding Source={StaticResource savocalc}, Path=Surface_cm2}" FontWeight="Bold" />
                                    <Label Content=" cm²" />
                                </StackPanel>
                                <StackPanel Orientation="Horizontal" Grid.Row="2" Grid.Column="2">
                                    <Label 
                                Name="tbSurface_m2"
                                Content="{Binding Source={StaticResource savocalc}, Path=Surface_m2, Converter={StaticResource FloatConsolidator}}" FontWeight="Bold" />
                                    <Label Content=" m²" />
                                </StackPanel>
                                <StackPanel Orientation="Horizontal" Grid.Row="3" Grid.Column="1">
                                    <Label 
                                Name="tbEfficiency" 
                                Content="{Binding Source={StaticResource savocalc}, Path=Efficiency}" FontWeight="Bold" />
                                    <Label Content="%" />
                                </StackPanel>
                                <Slider 
                                Name="slEfficiency"  
                                Grid.Row="3" 
                                Grid.Column="2"
                                Minimum="0" 
                                Maximum="100"
                                SmallChange="1"
                                LargeChange="10"
                                IsSnapToTickEnabled="True"
                                Value="{Binding Source={StaticResource savocalc}, Path=Efficiency}"
                                />
                            </Grid>
                            <Grid Grid.Column="1">
                                <Grid.ColumnDefinitions>
                                    <ColumnDefinition Width="1*" />
                                    <ColumnDefinition Width="1*" />
                                </Grid.ColumnDefinitions>
                                <Grid.RowDefinitions>
                                    <RowDefinition Height="Auto" />
                                    <RowDefinition Height="Auto" />
                                    <RowDefinition Height="Auto" />
                                    <RowDefinition Height="Auto" />
                                    <RowDefinition Height="1*" />
                                </Grid.RowDefinitions>
                                <Label Content="Teljesítmény:" Grid.Row="0" Grid.Column="0" />
                                <Label Content="Szélnyomás:" Grid.Row="1" Grid.Column="0" />
                                <Label Content="Fordulatszám:" Grid.Row="2" Grid.Column="0" />
                                <StackPanel Orientation="Horizontal" Grid.Row="0" Grid.Column="1">
                                    <Label 
                        Name="tbPower" 
                        Content="{Binding Source={StaticResource savocalc}, Path=Power, Converter={StaticResource FloatConsolidator}}" FontWeight="Bold" />
                                    <Label Content=" W" />
                                </StackPanel>
                                <StackPanel Orientation="Horizontal" Grid.Row="1" Grid.Column="1">
                                    <Label
                        Name="tbWindPressure" 
                        Content="{Binding Source={StaticResource savocalc}, Path=WindPressure, Converter={StaticResource FloatConsolidator}}" FontWeight="Bold" />
                                    <Label Content=" N" />
                                </StackPanel>
                                <StackPanel Orientation="Horizontal" Grid.Row="2" Grid.Column="1">
                                    <Label 
                        Name="tbRps"
                        Content="{Binding Source={StaticResource savocalc}, Path=Rps, Converter={StaticResource FloatConsolidator}}" FontWeight="Bold" />
                                    <Label Content=" Hz" />
                                </StackPanel>
                                <StackPanel Orientation="Horizontal" Grid.Row="3" Grid.Column="1">
                                    <Label 
                        Name="tbRpm" 
                        Content="{Binding Source={StaticResource savocalc}, Path=Rpm, Converter={StaticResource FloatConsolidator}}" FontWeight="Bold" />
                                    <Label Content=" Rpm" />
                                </StackPanel>
                            </Grid>
                        </StackPanel>
                    </Grid>
                </Grid>
                <Viewport3D Width="Auto" Height="Auto" Grid.Column="1">

                    <Viewport3D.Camera>
                        <PerspectiveCamera Position="100,100,100" LookDirection="-1,-1,-1" FieldOfView="45"/>
                    </Viewport3D.Camera>
                    <Viewport3D.Children>
                        <ModelVisual3D x:Name="Light">
                            <ModelVisual3D.Content>
                                <!--<AmbientLight />-->
                                <DirectionalLight Direction="1,-1,-0.5" Color="White"/>
                            </ModelVisual3D.Content>
                        </ModelVisual3D>
                        <ModelVisual3D x:Name="Light2">
                            <ModelVisual3D.Content>
                                <!--<AmbientLight />-->
                                <DirectionalLight Direction="-1,-1,-0.5" Color="White"/>
                            </ModelVisual3D.Content>
                        </ModelVisual3D>

                        <ModelVisual3D>

                            <ModelVisual3D.Transform>
                                <RotateTransform3D x:Name="trans3d">
                                    <RotateTransform3D.Rotation>
                                        <AxisAngleRotation3D
                                        x:Name="rotationY"
                                        Axis="0,1,0"
                                        Angle="{Binding Source={StaticResource savocalc}, Path=Angle}"
                                        />
                                    </RotateTransform3D.Rotation>
                                </RotateTransform3D>
                            </ModelVisual3D.Transform>


                            <ModelVisual3D.Content>
                                <Model3DGroup x:Name="Helix">
                                    <GeometryModel3D x:Name="Rotor1">
                                        <GeometryModel3D.Material>
                                            <DiffuseMaterial Brush="Blue"/>
                                        </GeometryModel3D.Material>
                                        <GeometryModel3D.Geometry>


